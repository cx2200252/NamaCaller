{
	"name": "video_face_info",
	"calls": [
		{
			"source":{
				"type": "image",
				"is_pick_file": true,
				"frame_repeat": [60, 1]
			},
			"output": {
				"type": "image",
				"dir": "E:/bs_optimise/video_face_info/tmp/",
				"name_format": "%06d.jpg",
				"out_flags": [[59, false], true]
			},
			"props":{
				"saver": "resources/face_info_saver.bundle"
			},
			"setting": [
				{
					"prop_name": "saver",
					"params": {
						"is_save": [[59, 0.0], 1.0],
						"save_path": "E:/bs_optimise/video_face_info/tmp/%06d",
						"dde_param": true,
						"ar_mesh": true,
						"neutral_id": true
					}
				}
			],
			"post_process":{
				"prop_1": {
					"type": "cmd",
					"cmd": "video_face_info_post_proc.bat",
					"params": "\"%source.dir%\" \"%output.dir%\"",
					"is_show": true
				}
			}
		}
	]
}