{
	"name": "image_face_info",
	"calls": [
		{
			"source":{
				"type": "image",
				"is_pick_file": true
			},
			"output": {
				"type": "image",
				"dir": "./image_face_info/",
				"name_format": "%05d.jpg"
			},
			"props":{
				"saver": "resources/face_info_saver.bundle"
			},
			"setting": [
				{
					"prop_name": "saver",
					"params": {
						"is_save": true,
						"save_path": "./image_face_info/%05d",
						"dde_param": true,
						"projected_mesh": true,
						"ar_mesh": true,
						"neutral_id": true,
						"bs": true
					}
				}
			]
		}
	]
}