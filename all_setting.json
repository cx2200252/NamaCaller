基本结构：

{
	"name": "123",
	"calls": [
		{
			"source":{
				"type": "camera",
				...
			},
			"need_reset": false,
			"props":{
				"prop_name1": "prop_path",
				...
			},
			"setting": [
				{
					"prop_name": "prop_name1",
					"params":{
						"param1": value,
						...
					}
				}
			],
			"addition_calls":[
				"call1": 类型1(addition_call_type)
			],
			"output": {
				"type": "image",
				...
			},
			"post_process":{
				"proc_1": {
					"type": "cmd",
					"cmd": "rm",
					"params": "..."
				}
			}
		},
		{

		}
	]
}


可用类型：
类型0(type)：[type, type] or [type, [1, type, type, ...], type, ...]
类型1(type)：[type, 3, type, 1, ...]

addition_call_type: 
{
	"is_before_render": true,<!-- default: true -->
	"func": "function_name",
	"params": [int, string]
}

func: 
	InitArdataExt(ardata_ext_path)


"source":{
	"type": "camera",
	"cam_id": 0,
	"width": 1280,
	"height": 720,
	"frame_cnt": 10,
	<!-- 输出帧数的上限 -->
	<!-- 未设置时： -->
	<!-- 源为图像：值为图像数加上因frame_repeat而多出来的帧数 -->
	<!-- 源为视频：值为视频帧数加上因frame_repeat而多出来的帧数 -->
	"frame_repeat": 类型0(int)
	<!-- 加起来帧数少于frame_cnt时，后续帧数使用最后一个值 -->
}
"source":{
	"type": "image",
	"path": string 或者 类型1(string),<!-- 类型1(string)时支持设置文件夹、文件，名称支持%d格式化 -->
	"is_vertical": true,
	"is_pick_file": false,
	"is_pick_folder": false,
	"frame_cnt": 10,
	"frame_repeat": 类型0(int)
}
"source":{
	"type": "video",
	"path": string
	"is_pick_file": false,
	"frame_cnt": 10,
	"frame_repeat": 类型0(int)
}


"props":{
	"prop_name1": 类型1(string),
	"prop_name2": 类型1(string)
}


"addition_calls":[
	"call1": 类型1(addition_call_type)
]

"setting": [
	{
		//使用通配符时，索引只使用会输出的索引，如前10帧不输出，产生参数的索引为-1；第11帧索引为0
		"prop_name": "prop_name1",
		"params":{
			"param1": "123",
			"param2": "%d",// 只支持整数
			"param3": int,
			"param4": true,
			"param5": {
				"from": 0.0,
				"step": 1.0,// 可选，未设置时，from-to映射到frame_cnt范围
				"to": 100.0
			}
			"param6": 类型0(float),
			"param7": 类型0(string)
		}		
	},
	{
		"prop_name": "prop_name2",
		"prop_name": 类型1(string)
		<!-- 计数少于frame_cnt时，使用最后一个 -->
	}
]


"output": {
	"type": "image",
	"dir": string,
	"name_format": "%d.jpg"
	<!-- 缺省为%03.jpg -->
	"names": 类型1(string)
	<!-- 有names的优先使用names，无names或names数目比frame_cnt少时，用name_format生成name -->
	"out_flags": 类型0(bool)
	<!-- 指定数目小于frame_cnt时，全部使用最后一个值 -->
}
"output": {
	"type": "video",
	"path": string,
	"fps": 25.0,
	<!-- 缺省为25.0，未设置且源为视频时，fps与源相同 -->
	"width": 1280,
	<!-- 宽高任一未设置，则为源的大小 -->
	"height": 720,
	<!-- 有names的优先使用names，无names或names数目比frame_cnt少时，用name_format生成name -->
	"out_flags": 类型0(bool)
	<!-- 指定数目小于frame_cnt时，全部使用最后一个值 -->
}


"post_process":{
	"proc_1": {
		"type": "cmd",
		"cmd": "命令",<!-- 必须为\\,不能为/ -->
		"params": "参数",<!-- 必须为\\,不能为/ -->
		"is_show": false,<!-- default: false -->
		"is_wait": true,<!-- default: true -->
	}
}
<!-- 参数：
源的第一张图or视频：
文件夹路径：%source.dir%
文件路径：%source.path%
文件夹名：%source.dir_name%

（假定）输出的第一张图or视频（忽略out_flags）：
文件夹路径：%output.dir%
文件路径（文件夹or文件）：%output.path%
文件夹名：%output.dir_name%


 -->